{
    "name": "mzh/hyperf-admin-plugin",
    "description": "开箱即用的Hyperf后台扩展，laravel-admin用法",
    "license": "MIT",
    "authors": [
        {
            "name": "毛自豪",
            "email": "4213509@qq.com"
        }
    ],
    "homepage": "https://github.com/lphkxd/hyperf-admin",
    "keywords": [
        "hyperf",
        "Admin",
        "Vue"
    ],
    "require": {
        "php": ">=7.3",
        "ext-swoole": ">=4.5",
        "mzh/hyperf-helper": "^0.1.7",
        "mzh/hyperf-swagger": "^1.0",
        "hyperf/filesystem": "^2.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mzh\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mzh\\Admin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mzh\\Admin\\ConfigProvider"
            ],
            "aliases": {
                "Admin": "Mzh\\Admin\\Facades\\Admin"
            }
        }
    }
}
